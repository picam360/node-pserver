[Unit]
Description = pserver daemon

[Service]
ExecStart = /usr/bin/docker /usr/bin/pserver -c @CONFIG_PATH@
ExecStop = -/usr/bin/docker kill pserver
Restart = always
RestartSec = 3
Type = simple

[Install]
WantedBy = multi-user.target